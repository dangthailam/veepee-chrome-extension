{
    "name": "Veepee extensions",
    "description": "Veepee extensions",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*.veepee.fr/*"
            ],
            "css": [
                "my-styles.css"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}